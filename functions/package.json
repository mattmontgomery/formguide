{
  "version": "1.0.0",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "ioredis": "^5.0.4",
    "node-fetch": "^3.2.4",
    "node-fetch-commonjs": "^3.1.1",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "build": "tsc",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./build/ --exec npm run start\"",
    "start": "functions-framework --source=build/src/ --target=$APPLICATION",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "test": "tsc --noEmit",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "deploy": "gcloud functions deploy form --trigger-http --runtime nodejs16 --allow-unauthenticated --region us-west3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "concurrently": "^7.1.0",
    "gts": "^3.1.0",
    "nodemon": "^2.0.16"
  },
  "main": "build/src/index.js"
}
